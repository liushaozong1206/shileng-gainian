{
    "presets": [
        "es2015",
        "react"
    ],
    "env": {
        "development": {
            "presets": [
                "react-hmre"
            ]
        },
        "production":{
            "plugins": [
                "transform-es3-property-literals",
                "transform-es3-member-expression-literals",
                "transform-es2015-modules-simple-commonjs"
            ]
        }
    }
}